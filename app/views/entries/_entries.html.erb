<table id="entries">
  <tr>
    <th>Date</th>
    <th></th>
    <th>Amount</th>
    <th></th>
    <th>Debit</th>
    <th>Credit</th>
    <th></th>
  </tr>
  <% entries.each do |entry| %>
  <tr>
    <td><%= entry.date.strftime "%d %b %y" %></td>
    <td><%= entry.currency %></td>
    <td><%= entry.amount %></td>
    <td><%= entry.entry_type %></td>
    <td><%= entry.debit_account.name %></td>
    <td><%= entry.credit_account.name %></td>
    <td><%= link_to "show", entry %></td>
  </tr>
  <% end %>
</table>
